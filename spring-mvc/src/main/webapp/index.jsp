<%@ page language="java" pageEncoding="UTF-8" contentType="text/html;charset=UTF-8" %>
<html>
	<head>
		<title>用户页面</title>
		<style type="text/css">
			*{
				margin: 0 auto;
			}
			#tab{
				margin-top:100px;
				border-top: 1px solid #CCC;
				border-left: 1px solid #CCC;
				width: 800px;
				cellspading:0
			}
			
			#tab td{
				border-right: 1px solid #CCC;
				border-bottom: 1px solid #CCC;
			}
			
			#tab tr{
				height: 30px;
			}
			
			
		</style>
	</head>
	
	<body>
		<table id="tab">
			<thead>
				<tr><td>ID</td><td>编码</td><td>操作</td></tr>
			</thead>
			<tbody>
				<tr><td>1</td><td>lsj</td><td>删除</td></tr>
			</tbody>
		</table>
	</body>
</html>